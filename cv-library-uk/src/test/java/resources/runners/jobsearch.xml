<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="testsuite" verbose="1">
    <test name="JobSearch Test">
        <parameter name="jobTitle" value="Tester"></parameter>
        <parameter name="location" value="Harrow"></parameter>
        <parameter name="distance" value="5 miles"></parameter>
        <parameter name="salaryMin" value="30000"></parameter>
        <parameter name="salaryMax" value="500000"></parameter>
        <parameter name="salaryType" value="Per annum"></parameter>
        <parameter name="jobType" value="Permanent"></parameter>
        <parameter name="result" value="Permanent Tester jobs in Harrow"></parameter>
        <classes>
            <class name="uklibrary.testsuite.JobSearchTest"></class>
        </classes>
    </test>
</suite>